{
	"$schema": "http://json-schema.org/schema#",
	"description": "Schema for a new ticket being opened",
	"self": {
		"vendor": "com.zendesk.zendesk",
		"name": "ticket_opened",
		"format": "jsonschema",
		"version": "1-0-0"
	},

	"type": "object",
	"properties": {
		"ticket.account": {
			"type": "string"
		},
		"ticket.cc_names": {
			"type": "string"
		},
		"ticket.created_at": {
			"type": "string"
		},
		"ticket.created_at_with_timestamp": {
			"type": "string"
		},
		"ticket.due_date": {
			"type": "string"
		},
		"ticket.due_date_with_timestamp": {
			"type": "string"
		},
		"ticket.external_id": {
			"type": "string"
		},
		"ticket.id": {
			"type": "string"
		},
		"ticket.in_business_hours": {
			"type": "boolean"
		},
		"ticket.priority": {
			"enum": ["Low", "Normal", "High", "Urgent"]
		},
		"ticket.score": {
			"type": "number"
		},
		"ticket.status": {
			"enum": ["New", "Open", "Pending", "Solved"]
		},
		"ticket.tags": {
			"type": "string"
		},
		"ticket.ticket_type": {
			"type": "string"
		},
		"ticket.title": {
			"type": "string"
		},
		"ticket.updated_at": {
			"type": "string"
		},
		"ticket.updated_at_with_timestamp": {
			"type": "string"
		},
		"ticket.url": {
			"type": "string",
		},
		"ticket.url_with_protocol": {
			"type": "string",
		},
		"ticket.via": {
			"type": "string"
		}
	},
	"minProperties": 1,
	"additionalProperties": false
}
